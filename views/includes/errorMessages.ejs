<% if(locals.errors){ %> <% errors.forEach(function(error) { %>
  <article class="message">
    <div class="message-header">
      <p>Error</p>
      <button class="delete" aria-label="delete"></button>
    </div>
    <div class="message-body">
      <%= error.msg %>
    </div>
  </article>
<% }); %>
<% } %> 